   "source": [
    "import pandas as pd\n",
    "df = pd.read_csv(\"assets/Hitters.csv\")\n",
    "BinaryLeague = []\n",
    "for league in df[\"League\"]:\n",
    "    if(league == \"A\"):\n",
    "        BinaryLeague.append(1)\n",
    "    else:\n",
    "        BinaryLeague.append(0)\n",
    "df[\"BinaryLeague\"] = BinaryLeague"
   ]
